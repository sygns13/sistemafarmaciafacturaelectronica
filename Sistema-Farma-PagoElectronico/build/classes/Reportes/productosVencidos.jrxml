	<queryString>
		<![CDATA[select l.idlote,p.nomproducto, p.codunid, c.nomtip,p.composicion, pre.present, m.nommarc, a.nombre as almacen,l.nombre, date_format(l.fecven,'%d/%m/%Y') as fecha
from lote l
inner join produto p on p.idproduto=l.idproducto
inner join tipoproducto c on c.idtipoproducto=p.idtipoproducto
inner join presentacion pre on pre.idpresentacion=p.idpresentacion
inner join marca m on m.idmarca=p.idmarca
inner join stock s on s.idproducto=p.idproduto
inner join almacen a on a.idalmacen=s.idalmacen
where $P!{andfecha} and l.cant>0 and
(p.codUnid like '%$P!{bus}%' or nomproducto like '%$P!{bus}%' or m.nommarc like '%$P!{bus}%')
group by l.idlote
order by l.fecven ;]]>
	</queryString>
