	<queryString>
		<![CDATA[(select p.idproduto as codctlg, p.nomproducto as nomctlg, p.fecha as fecingralm,

FORMAT((SELECT IFNULL((p.preciocompra*(select s.cantidaddisponible-
(select Ifnull((select sum(d.cantidad*d.cantReal) from detalleentradastock d inner join entradastock e on e.idEntradaStock=d.idEntradaStock  where d.idproducto=s.idproducto and e.actualizado='1' group by d.idproducto),0))+
(select Ifnull((select sum(dv.cantidad*dv.cantReal) from detalleventa dv inner join venta v on v.idventa=dv.idventa where dv.idproducto=s.idproducto and v.estado<>'Anulado'  group by dv.idproducto),0))
from stock s where s.idproducto=p.idproduto))/(select s.cantidaddisponible-
(select Ifnull((select sum(d.cantidad*d.cantReal) from detalleentradastock d inner join entradastock e on e.idEntradaStock=d.idEntradaStock  where d.idproducto=s.idproducto and e.actualizado='1' group by d.idproducto),0))+
(select Ifnull((select sum(dv.cantidad*dv.cantReal) from detalleventa dv inner join venta v on v.idventa=dv.idventa where dv.idproducto=s.idproducto and v.estado<>'Anulado'  group by dv.idproducto),0))
from stock s where s.idproducto=p.idproduto),0)),2) as costo,
concat('Inventario Inicial')as ingres,
(select s.cantidaddisponible-
(select Ifnull((select sum(d.cantidad*d.cantReal) from detalleentradastock d  inner join entradastock e on e.idEntradaStock=d.idEntradaStock  where d.idproducto=s.idproducto and e.actualizado='1' group by d.idproducto),0))+
(select Ifnull((select sum(dv.cantidad*dv.cantReal) from detalleventa dv inner join venta v on v.idventa=dv.idventa where dv.idproducto=s.idproducto and v.estado<>'Anulado'  group by dv.idproducto),0))
from stock s where s.idproducto=p.idproduto) as canting,
0.00  as cantisal,
0.00 as precio,
p.preciocompra*(select format(s.cantidaddisponible,2)-
(select Ifnull((select sum(d.cantidad*d.cantReal) from detalleentradastock d inner join entradastock e on e.idEntradaStock=d.idEntradaStock  where d.idproducto=s.idproducto and e.actualizado='1' group by d.idproducto),0))+
(select Ifnull((select sum(dv.cantidad*dv.cantReal) from detalleventa dv inner join venta v on v.idventa=dv.idventa where dv.idproducto=s.idproducto and estado<>'Anulado'  group by dv.idproducto),0))
from stock s where s.idproducto=p.idproduto) as totaling, 0.00 as totalsal
from produto p
where p.idproduto=$P{id}
)
union
(
select p.idproduto as codctlg, p.nomproducto as nomctlg, e.fecha as fecingralm,
format(de.costo/de.cantReal,2) as costo,
(select ifnull((select concat('Ingresos',' ',t.nombre,' ', f.`serie`,' - ',  f.`numero`) from
facturaproveedor f
inner join tipocomprobante t on t.idtipocomprobante=f.idtipocomprobante where f.idfacturaproveedor=e.idfacturaproveedor),'Ingreso Libre no Facturado')) as ingres
,
(de.cantidad*de.cantReal)as canting, 0.00  as cantisal, 0.00 as precio,
de.costo*de.cantidad as totaling, 0.00 as totalsal
from produto p
inner join detalleentradastock de on de.idproducto=p.idproduto
inner join entradastock e on e.identradastock=de.identradastock and e.actualizado='1'
where p.idproduto=$P{id}
)
union (select p.idproduto as codctlg, p.nomproducto as nomctlg, v.fecha as fecingralm,
0.00,
(select ifnull((select concat('Salidas',' ',t.nombre,' ', c.`serie`,' - ',  c.`numero`) from
comprobante c
inner join tipocomprobante t on t.idtipocomprobante=c.idtipocomprobante where c.idcomprobante=v.idcomprobante),'Salida Libre')) as ingres
,
00 as canting, (de.cantidad*de.cantReal)  as cantisal, format((de.precioventa/de.cantReal),2) as precio,
00 as totaling, de.cantidad*de.precioventa as totalsal
from produto p
inner join detalleventa de on de.idproducto=p.idproduto
inner join venta v on v.idventa=de.idventa  and estado<>'Anulado'
where p.idproduto=$P{id})
order by 2,3,5 asc;]]>
	</queryString>
