	<queryString>
		<![CDATA[SELECT FORMAT(@a:= v.`subTotalInafecto`+v.`subTotalAfecto`+ v.`igv`,2) AS totalito, (@b:=(SELECT IFNULL(SUM(cv.`importe`),0.00) As montoCobrado FROM `cobroventa` cv WHERE cv.`idVenta`=v.`idVenta`)) AS montoCobrado, (@a-@b) AS porCobrar,vt.`Descripcion`, d.`cantidad`, (d.`precioVenta`- d.`descuento`) as precioVenta, CAST((d.`cantidad`*(d.`precioVenta`- d.`descuento`)) AS DECIMAL(9,2)) AS total
FROM vta_catalogo vt, detalleventa d,venta v
WHERE d.`idProducto`=vt.`ID` AND v.`idVenta`= d.`idVenta` AND d.`idVenta`= $P{idVenta}
union
SELECT FORMAT(@a:= v.`subTotalInafecto`+v.`subTotalAfecto`+ v.`igv`,2),(@b:=(SELECT IFNULL(SUM(cv.`importe`),0.00) As montoCobrado FROM `cobroventa` cv WHERE cv.`idVenta`=v.`idVenta`)),(@a-@b),'En Obra',-1,enObra,enObra
from venta v where v.idventa= $P{idVenta}  and v.enobra!=-1;]]>
	</queryString>
