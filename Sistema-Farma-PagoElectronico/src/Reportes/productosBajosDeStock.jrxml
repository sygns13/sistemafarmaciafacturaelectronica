	<queryString>
		<![CDATA[select p.idProduto,p.codUnid, p.nomproducto, c.nomtip, p.composicion, pre.present, m.nommarc, un.nomuniddes, a.nombre,
p.stockMinimo, s.cantidadDisponible
from produto p
inner join tipoproducto c on c.idTipoProducto=p.idTipoProducto
inner join presentacion pre on pre.idPresentacion=p.idPresentacion
inner join marca m on m.idmarca=p.idmarca
inner join detunidprod u on u.idproducto=p.idproduto and idUnidades='1'
inner join unidades un on un.idUnidades=u.idunidades
inner join stock s on s.idProducto=p.idproduto
inner join almacen a on a.idalmacen=s.idAlmacen
WHERE  s.cantidadDisponible<=p.stockMinimo and
(p.codUnid like '%$P!{bus}%' or nomproducto like '%$P!{bus}%' or m.nommarc like '%$P!{bus}%')
group by p.idProduto, a.idAlmacen ORDER BY p.`idproduto`;]]>
	</queryString>
