	<queryString>
		<![CDATA[SELECT FORMAT(@a:= v.`subTotalInafecto`+v.`subTotalAfecto`+ v.`igv`,2) AS totalito, (@b:=(SELECT IFNULL(SUM(cv.`importe`),0.00) As montoCobrado
FROM `cobroventa` cv WHERE cv.`idVenta`=v.`idVenta`)) AS montoCobrado, (@a-@b) AS porCobrar,concat(d.Unidad,' ',pre.`present`,' ', p.nomproducto)
AS Descripcion, d.`cantidad`, format(d.`precioVenta`,2) as precioVenta, d.descuento,
format(((d.cantidad*d.cantreal)*(d.precioventa-(d.precioventa*(d.descuento/100)))),2) AS total
FROM vta_catalogo vt, detalleventa d
inner join produto p on p.idproduto=d.idproducto
inner join presentacion pre on pre.idpresentacion=p.idpresentacion
,venta v
WHERE d.`idProducto`=vt.`ID` AND v.`idVenta`= d.`idVenta` AND d.`idVenta`= $P{idVenta};]]>
	</queryString>
