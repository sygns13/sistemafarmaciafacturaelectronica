	<queryString>
		<![CDATA[select date_format(a.`fecha`,'%d/%m/%Y') as fecha,
ifnull((select replace(format(sum(dv.cantidad*(dv.precioVenta-((dv.precioVenta*descuento)/100))),'2'),',','')
from venta v
inner join detalleventa dv on v.idventa=dv.idventa
where v.fecha=a.fecha),0.00) as suma,
ifnull((select replace(format(sum(dv.cantidad*(dv.precioVenta-((dv.precioVenta*descuento)/100))),'2'),',','')
from venta v
inner join detalleventa dv on v.idventa=dv.idventa
where v.fecha=a.fecha),0.00) as suma2
from auxfecha a
where a.fecha>='2018/02/01'
limit 10;]]>
	</queryString>
